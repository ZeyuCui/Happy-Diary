<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.happydiaryy">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/appicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.HappyDiaryy">

        <activity
            android:name="com.example.happydiary.ChangeThemeActivity"
            android:exported="false" />

        <activity
            android:name="com.example.happydiary.CalendarMainActivity"
            android:exported="false"/>


        <activity android:name="com.example.happydiary.ReadActivity"
            android:exported="false"/>
        <activity android:name="com.example.happydiary.Zeyu.ListActivity"
            android:exported="false"/>
        <activity
            android:name="com.example.happydiary.EditActivity"
            android:exported="false" />
        <activity
            android:name="com.example.happydiary.CreateActivity"
            android:exported="false" />
        <activity
            android:name="com.example.happydiary.Settings"
            android:exported="false" />
        <activity
            android:name="com.example.happydiary.ForgotPassword"
            android:exported="false" />
        <activity
            android:name="com.example.happydiary.CreateAccount"
            android:exported="false" />
        <activity
            android:name="com.example.happydiary.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.example.happydiary.SignIn"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>